    return {v, nack};
  }
  METAL_FUNC checked_color<T> check_sample(sampler s, float2 coord, uint array, bias bias_options, min_lod_clamp min_lod_clamp_options, int2 offset = int2(0)) const thread
  {
    // TODO: Using static assert prevents usage of SFINAE. We need to remove it
    // -- see <rdar://problem/31769738>.
    static_assert(_is_valid_sample(a), _err_sample_access_msg);
    bool nack;
    T v = __metal_sample_depth_2d_array_t(t, s.val, coord, array, true, offset, false, bias_options.value, min_lod_clamp_options.value, int(a), &nack);
    return {v, nack};
  }
#endif
  METAL_FUNC checked_color<T> check_sample(sampler s, float2 coord, uint array, gradient2d options, int2 offset = int2(0)) const thread
  {
    // TODO: Using static assert prevents usage of SFINAE. We need to remove it
    // -- see <rdar://problem/31769738>.
    static_assert(_is_valid_sample(a), _err_sample_access_msg);
    bool nack;
    T v = __metal_sample_depth_2d_array_t_grad(t, s.val, coord, array, options.dPdx, options.dPdy, true, offset, 0.0f, int(a), &nack);
    return {v, nack};
  }
