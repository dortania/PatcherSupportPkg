  }
#if defined(__HAVE_MIN_LOD_CLAMP__)
  METAL_FUNC checked_color<T> check_sample(sampler s, float2 coord, gradient2d grad_options, min_lod_clamp min_lod_clamp_options, int2 offset = int2(0)) const thread
