  }
#endif
#endif
#if defined(__HAVE_INDIRECT_ARGUMENT_BUFFER__)
#if defined(__HAVE_16B_COORDS__)
  METAL_FUNC vec<T, 4> read(ushort3 coord, ushort lod = 0) const constant
  {
