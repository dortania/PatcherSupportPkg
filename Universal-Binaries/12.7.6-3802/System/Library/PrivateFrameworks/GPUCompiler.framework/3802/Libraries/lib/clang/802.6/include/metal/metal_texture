    return {v, nack};
  }
  METAL_FUNC checked_color<vec<T, 4>> check_sample(sampler s, float3 coord, bias bias_options, min_lod_clamp min_lod_clamp_options) const thread
  {
    // TODO: Using static assert prevents usage of SFINAE. We need to remove it
