  }
  METAL_FUNC checked_color<T> check_sample(sampler s, float2 coord, bias bias_options, min_lod_clamp min_lod_clamp_options, int2 offset = int2(0)) const thread
  {
    // TODO: Using static assert prevents usage of SFINAE. We need to remove it
