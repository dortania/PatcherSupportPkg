    return __metal_sample_depth_2d_t_grad(t, s.val, coord, grad_options.dPdx, grad_options.dPdy, true, offset, min_lod_clamp_options.value, int(a), static_cast<thread bool*>(nullptr));
  }
#endif
#if defined (__HAVE_SPARSE_TEXTURES__)
  METAL_FUNC checked_color<T> check_sample(sampler s, float2 coord, int2 offset = int2(0)) const thread
  {
    // TODO: Using static assert prevents usage of SFINAE. We need to remove it
    // -- see <rdar://problem/31769738>.
    static_assert(_is_valid_sample(a), _err_sample_access_msg);
    bool nack;
    T v = __metal_sample_depth_2d_t(t, s.val, coord, true, offset, false, 0.0f, 0.0f, int(a), &nack);
    return {v, nack};
  }
  METAL_FUNC checked_color<T> check_sample(sampler s, float2 coord, bias options, int2 offset = int2(0)) const thread
  {
    // TODO: Using static assert prevents usage of SFINAE. We need to remove it
    // -- see <rdar://problem/31769738>.
    static_assert(_is_valid_sample(a), _err_sample_access_msg);
    bool nack;
    T v = __metal_sample_depth_2d_t(t, s.val, coord, true, offset, false, options.value, 0.0f, int(a), &nack);
    return {v, nack};
  }
  METAL_FUNC checked_color<T> check_sample(sampler s, float2 coord, level options, int2 offset = int2(0)) const thread
  {
    // TODO: Using static assert prevents usage of SFINAE. We need to remove it
    // -- see <rdar://problem/31769738>.
    static_assert(_is_valid_sample(a), _err_sample_access_msg);
